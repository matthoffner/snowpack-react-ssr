    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.55",
    "@material-ui/styles": "^4.10.0",
    "apollo-boost": "^0.4.9",
    "apollo-client": "^2.6.10",
    "clsx": "^1.1.1",
    "graphql": "^15.1.0",
    "http2": "^3.3.7",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.7.2",
